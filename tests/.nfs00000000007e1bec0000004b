#!/bin/bash

rm -f coube.txt
rm -f courbeopt.txt
for i in `seq 1 50`;
do
    ./random_input.pl $i 100 > req.txt
    (time ./words < req.txt > /dev/null) 2> courbe1.txt
    (time ./optimisation/wordso < req.txt > /dev/null) 2> courbe2.txt
    echo $((50 - $i))
    cat courbe1.txt | grep  "real" | cut -d "s" -f 1 >> courbe.txt
    cat courbe2.txt | grep  "real" | cut -d "s" -f 1 >> optimisation/courbeopt.txt
done
echo "fini!"
rm -f courbe1.txt
rm -f courbe2.txt